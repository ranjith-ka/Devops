{
    "configurations": [
        {
            "name": "File Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "showLog": true
        },
        {
            "name": "serve",
            "type": "go",
            "request": "launch",
            "program": "${fileDirname}/..",
            "env": {},
            "args": [
                "serve"
            ]
        },
        {
            "name": "verify",
            "type": "go",
            "request": "launch",
            "program": "${fileDirname}/..",
            "env": {},
            "args": [
                "verify"
            ]
        },
        {
            "name": "random",
            "type": "go",
            "request": "launch",
            "program": "${fileDirname}/..",
            "env": {},
            "args": [
                "random"
            ]
        },
        {
            "name": "Skaffold Debug",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "host": "localhost",
            "port": 56268,
            "cwd": "${workspaceFolder}",
            "remotePath": "/go"
        }
    ]
}